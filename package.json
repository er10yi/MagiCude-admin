    "file-saver": "^2.0.2",
    "js-cookie": "2.2.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "moment": "^2.27.0",
    "normalize.css": "8.0.1",
    "npm-check-updates": "^9.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.1.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "v-charts": "^1.19.0",
    "vue": "2.6.12",
    "vue-count-to": "^1.0.13",
    "vue-cron-generator": "^0.1.9",
    "vue-router": "3.4.3",
