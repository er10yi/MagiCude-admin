    "@vue/cli-plugin-unit-jest": "4.5.6",
    "@vue/cli-service": "4.5.6",
    "@vue/test-utils": "1.1.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "chalk": "4.1.0",
    "connect": "3.7.0",
    "css-loader": "^4.3.0",
    "eslint": "7.9.0",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "4.4.1",
